{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "ES2020",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "tsBuildInfoFile": "./.next/.tsbuildinfo",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@core/*": ["../../packages/core/src/*"],
      "@examples/*": ["../examples/*"],
      "@lucky/contracts": ["../../packages/contracts/src"],
      "@lucky/contracts/*": ["../../packages/contracts/src/*"],
      "@lucky/core": ["../../packages/core/src"],
      "@lucky/core/*": ["../../packages/core/src/*"],
      "@lucky/examples": ["../examples"],
      "@lucky/examples/*": ["../examples/*"],
      "@lucky/experiments": ["../../packages/experiments/src"],
      "@lucky/experiments/*": ["../../packages/experiments/src/*"],
      "@lucky/shared": ["../../packages/shared/src"],
      "@lucky/shared/*": ["../../packages/shared/src/*"],
      "@lucky/tools": ["../../packages/tools/src"],
      "@lucky/tools/*": ["../../packages/tools/src/*"]
    },
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    ".next/types/**/*.ts",
    "src/core/utils/db/supabase/types.ts"
  ],
  "exclude": [
    "node_modules",
    "../examples/logging_folder",
    "../../packages/experiments/src/experiments-2/**",
    ".next/**",
    "src/lib/scraping/**",
    "src/results/**"
  ]
}
