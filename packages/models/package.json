{
  "name": "@lucky/models",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "Multi-gateway model configuration and registry for Vercel AI SDK",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": ["dist"],
  "scripts": {
    "build": "tsup --silent",
    "prebuild": "bun run scripts/validate-catalog.ts",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "fetch-pricing": "bun run scripts/fetch-pricing.ts",
    "validate-configs": "bunx tsx scripts/validate-yaml-configs.ts",
    "validate-catalog": "bun run scripts/validate-catalog.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "exports": {
    ".": {
      "browser": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./llm-catalog/catalog-queries": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "ts": "./src/llm-catalog/catalog-queries.ts"
    },
    "./llm-catalog/catalog": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "ts": "./src/llm-catalog/catalog.ts"
    },
    "./llm-catalog/providers": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "ts": "./src/llm-catalog/providers.ts"
    },
    "./llm-catalog/defaults": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "ts": "./src/llm-catalog/defaults.ts"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "node": {
        "import": "./dist/server.js"
      },
      "ts": "./src/server.ts"
    },
    "./config": {
      "types": "./dist/config/define.d.ts",
      "node": {
        "import": "./dist/config/define.js"
      },
      "ts": "./src/config/define.ts"
    },
    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "import": "./dist/middleware/index.js",
      "ts": "./src/middleware/index.ts"
    },
    "./pricing/catalog": {
      "types": "./dist/pricing/catalog.d.ts",
      "import": "./dist/pricing/catalog.js",
      "ts": "./src/pricing/catalog.ts"
    },
    "./utils/apikey": {
      "types": "./dist/utils/apikey.d.ts",
      "import": "./dist/utils/apikey.js",
      "ts": "./src/utils/apikey.ts"
    }
  },
  "dependencies": {
    "@lucky/shared": "workspace:*",
    "yaml": "^2.3.4",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^18.19.0",
    "@typescript/native-preview": "^7.0.0-dev.20251009.1",
    "prettier": "^3.4.2",
    "rimraf": "^5.0.10",
    "tsup": "8.5.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "ai": ">=5.0.0",
    "@ai-sdk/openai": ">=2.0.0",
    "@ai-sdk/anthropic": ">=2.0.0",
    "@ai-sdk/groq": ">=2.0.0",
    "@ai-sdk/openai-compatible": ">=1.0.0",
    "@openrouter/ai-sdk-provider": ">=1.2.0"
  }
}
