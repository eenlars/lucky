{
  "name": "@lucky/models",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "description": "Multi-provider model configuration and registry for Vercel AI SDK",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": ["dist"],
  "scripts": {
    "build": "tsup --silent",
    "prebuild": "bun run scripts/validate-catalog.ts",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "fetch-pricing": "bun run scripts/fetch-pricing.ts",
    "validate-configs": "bunx tsx scripts/validate-yaml-configs.ts",
    "validate-catalog": "bun run scripts/validate-catalog.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config/define.d.ts",
      "import": "./dist/config/define.js"
    },
    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "import": "./dist/middleware/index.js"
    },
    "./pricing/catalog": {
      "types": "./dist/pricing/catalog.d.ts",
      "import": "./dist/pricing/catalog.js"
    }
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.27",
    "@ai-sdk/groq": "^2.0.24",
    "@ai-sdk/openai": "^2.0.0",
    "@ai-sdk/openai-compatible": "^1.0.22",
    "@lucky/shared": "workspace:*",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "ai": "^5.0.58",
    "yaml": "^2.3.4",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^18.19.0",
    "@typescript/native-preview": "^7.0.0-dev.20251009.1",
    "prettier": "^3.4.2",
    "rimraf": "^5.0.10",
    "tsup": "8.5.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "ai": ">=5.0.0"
  }
}
