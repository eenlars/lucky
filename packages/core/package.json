{
  "name": "@together/core",
  "version": "0.1.0",
  "private": true,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./utils/*": {
      "import": "./lib/utils/*.js",
      "types": "./lib/utils/*.d.ts"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc -b .",
    "typecheck": "tsc -b .",
    "lint": "eslint src",
    "format": "prettier --write .",
    "once": "tsx --env-file=.env src/runOnce.ts",
    "twice": "tsx --env-file=.env src/runTwice.ts",
    "gp": "node scripts/run-gp.js",
    "cult": "rm -rf src/runtime/logging_folder && tsx --env-file=.env src/main.ts --mode=cultural",
    "clean": "tsx --env-file=.env scripts/cleanup.ts",
    "test": "bunx vitest",
    "test:core": "bunx vitest src",
    "test:integration": "bunx vitest run --config vite.integration.getSettings().ts",
    "coverage": "bunx vitest run --coverage",
    "gen:types": "supabase gen types typescript --project-id qnvprftdorualkdyogka --schema public > src/utils/clients/supabase/types.ts",
    "tree": "tree -L 7 -I 'node_modules|.git|dist|build|public' src",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/groq": "^1.2.9",
    "@ai-sdk/openai": "^1.3.23",
    "@dagrejs/dagre": "^1.1.5",
    "@huggingface/hub": "^2.4.0",
    "@mendable/firecrawl-js": "^1.29.1",
    "@modelcontextprotocol/sdk": "^1.15.1",
    "@openrouter/ai-sdk-provider": "0.7.2",
    "@playwright/mcp": "^0.0.29",
    "@supabase/supabase-js": "^2.50.5",
    "@types/glob": "^9.0.0",
    "@types/lodash": "^4.17.20",
    "@types/papaparse": "^5.3.16",
    "@types/which": "^3.0.4",
    "ai": "^4.3.17",
    "ajv": "^8.17.1",
    "bottleneck": "^2.19.5",
    "boxen": "^8.0.1",
    "bun": "^1.2.18",
    "chalk": "^5.4.1",
    "cheerio": "^1.1.0",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "elkjs": "^0.10.0",
    "express": "^5.1.0",
    "fastmcp": "^3.8.4",
    "figlet": "^1.8.1",
    "fuse.js": "^7.1.0",
    "glob": "^11.0.3",
    "gradient-string": "^3.0.0",
    "helmet": "^8.1.0",
    "inquirer": "^12.7.0",
    "json-canonicalize": "^2.0.0",
    "json5": "^2.2.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lru-cache": "^11.1.0",
    "mem0ai": "^2.1.36",
    "nanoid": "^5.1.5",
    "openai": "^5.10.1",
    "ora": "^8.2.0",
    "p-timeout": "^6.1.4",
    "papaparse": "^5.5.3",
    "parquet-wasm": "^0.6.1",
