{
  "name": "@lucky/core",
  "version": "0.1.0",
  "private": true,
  "description": "Core workflow engine, evaluation, and evolution algorithms",
  "exports": {
    "./package.json": "./package.json",
    "./core-config": "./src/core-config/coreConfig.ts",
    "./models/*": "./src/models/*",
    "./workflow/*": "./src/workflow/*",
    "./utils/*": "./src/utils/*",
    "./messages/*": "./src/messages/*",
    "./prompts/*": "./src/prompts/*",
    "./evaluation/*": "./src/evaluation/*",
    "./improvement/*": "./src/improvement/*",
    "./node/*": "./src/node/*",
    "./tools/*": "./src/tools/*",
    "./clients/*": "./src/clients/*"
  },
  "scripts": {
    "dev": "vitest --watch --project unit",
    "build": "tsc --noEmit",
    "start": "bun run src/main.ts",
    "list": "node scripts/dev.js list",
    "once": "bun run scripts/runOnce.ts",
    "gp": "node scripts/run-gp.js",
    "iterative": "rm -rf .core-data && bun run src/main.ts --mode=iterative",
    "clean": "bun run scripts/cleanup.ts",
    "test": "bunx vitest --project unit",
    "test:unit": "bunx vitest run --project unit",
    "test:core": "bunx vitest --project unit",
    "test:integration": "bunx vitest --project integration",
    "coverage": "bunx vitest run --coverage",
    "tree": "tree -L 7 -I 'node_modules|.git|dist|build|public' src/core",
    "tsc": "tsc --noEmit",
    "pricing": "bun run scripts/fetchOpenRouterPricing.ts"
  },
  "dependencies": {
    "@ai-sdk/groq": "^2.0.0",
    "@ai-sdk/openai": "^2.0.0",
    "@huggingface/hub": "^2.4.0",
    "@lucky/models": "workspace:*",
    "@lucky/shared": "workspace:*",
    "@lucky/tools": "workspace:*",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@supabase/supabase-js": "^2.50.5",
    "@t3-oss/env-nextjs": "^0.13.8",
    "ai": "^5.0.58",
    "bottleneck": "^2.19.5",
    "chalk": "^5.4.1",
    "dotenv": "^17.2.1",
    "lodash": "^4.17.21",
    "mem0ai": "^2.1.38",
    "nanoid": "^5.1.5",
    "p-timeout": "^6.1.4",
    "parquet-wasm": "^0.6.1",
    "zod": "^3.25.73",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@modelcontextprotocol/server-filesystem": "^2025.7.1",
    "@types/node": "^24.0.13",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.30.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "firecrawl-mcp": "^1.12.0",
    "prettier": "^3.6.2",
    "tavily-mcp": "0.2.7",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "type": "module"
}
