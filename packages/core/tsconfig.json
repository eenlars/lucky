{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@/core/*": ["./src/*"],
      "@/types/*": ["./src/types/*"],
      "@/logger": ["./src/utils/logging/Logger.ts"],
      "@/benchmarks/*": ["./src/workflow/ingestion/benchmarks/*"],
      "@/runtime/settings/constants": ["./src/runtime/settings/constants.ts"],
      "@/runtime/settings/constants.client": ["./src/runtime/settings/constants.client.ts"],  
      "@/runtime/settings/models": ["./src/runtime/settings/models.ts"],
      "@/runtime/settings/inputs": ["./src/runtime/settings/inputs.ts"],
      "@/runtime/schemas/location.types": ["./src/runtime/schemas/location.types.ts"],
      "@/runtime/code_tools/file-saver/save": ["./src/runtime/code_tools/file-saver/save.ts"],
      "@/runtime/code_tools/todo-manager/tool-todo-write": ["./src/runtime/code_tools/todo-manager/tool-todo-write.ts"],
      "@/runtime/code_tools/todo-manager/tool-todo-read": ["./src/runtime/code_tools/todo-manager/tool-todo-read.ts"],
      "@workflow/*": ["./src/workflow/*"],
      "@node/*": ["./src/node/*"],
      "@messages/*": ["./src/messages/*"],
      "@memories/*": ["./src/memories/*"],
      "@tools/*": ["./src/tools/*"],
      "@prompts/*": ["./src/prompts/*"],
      "@validation/*": ["./src/utils/validation/*"],
      "@improvement/*": ["./src/improvement/*"],
      "@behavioral/*": ["./src/improvement/behavioral/*"],
      "@gp/*": ["./src/improvement/gp/*"]
    },
    "allowSyntheticDefaultImports": true
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.js"],
  "exclude": ["node_modules"]
}
