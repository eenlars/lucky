{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@/core/*": ["./src/core/*"],
      "@/types/*": ["./src/types/*"],
      "@/logger": ["./src/core/utils/logging/Logger.ts"],
      "@/benchmarks/*": ["./src/core/workflow/ingestion/benchmarks/*"],
      "@workflow/*": ["./src/core/workflow/*"],
      "@node/*": ["./src/core/node/*"],
      "@messages/*": ["./src/core/messages/*"],
      "@memories/*": ["./src/core/memories/*"],
      "@tools/*": ["./src/core/tools/*"],
      "@prompts/*": ["./src/core/prompts/*"],
      "@validation/*": ["./src/core/utils/validation/*"],
      "@improvement/*": ["./src/core/improvement/*"],
      "@behavioral/*": ["./src/core/improvement/behavioral/*"],
      "@gp/*": ["./src/core/improvement/gp/*"]
    },
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    ".next/types/**/*.ts",
    "src/core/utils/db/supabase/types.ts"
  ],
  "exclude": ["node_modules", "src/runtime/logging_folder"]
}
